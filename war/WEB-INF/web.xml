		<servlet-name>ThankYouPage</servlet-name>
		<jsp-file>/WEB-INF/ThankYou.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>ThankYouPage</servlet-name>
		<url-pattern>/thankYou</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>UploadPage</servlet-name>
		<jsp-file>/WEB-INF/Upload.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>UploadPage</servlet-name>
		<url-pattern>/upload</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>AccountSettingsPage</servlet-name>
		<jsp-file>/WEB-INF/AccountSettings.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>AccountSettingsPage</servlet-name>
		<url-pattern>/accountSet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>FeedbackPage</servlet-name>
