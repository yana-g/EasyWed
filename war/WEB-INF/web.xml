		<servlet-name>InboxPage</servlet-name>
		<jsp-file>/WEB-INF/Inbox.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>InboxPage</servlet-name>
		<url-pattern>/inbox</url-pattern>
	</servlet-mapping>	

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>WEB-INF/Index.jsp</welcome-file>
	</welcome-file-list>



</web-app>