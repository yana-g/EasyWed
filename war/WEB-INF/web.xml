<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">

	<!-- Servlets -->
	<servlet>
		<servlet-name>Registration</servlet-name>
		<servlet-class>usersPack.Registration</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Registration</servlet-name>
		<url-pattern>/Registration</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>usersPack.Login</servlet-class> <!--    .java -->
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/LogedIn</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Search</servlet-name>
		<servlet-class>searchPack.Search</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Search</servlet-name>
		<url-pattern>/SearchRes</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>IndexPage</servlet-name>
		<jsp-file>/WEB-INF/Index.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>IndexPage</servlet-name>
		<url-pattern>/index</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ContactPage</servlet-name>
		<jsp-file>/WEB-INF/Contact.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>ContactPage</servlet-name>
		<url-pattern>/contact</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>LoginPage</servlet-name>
		<jsp-file>/WEB-INF/Login.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginPage</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ProfessionalPage</servlet-name>
		<jsp-file>/WEB-INF/Professional.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProfessionalPage</servlet-name>
		<url-pattern>/professional</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ProfilePage</servlet-name>
		<jsp-file>/WEB-INF/Profile.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProfilePage</servlet-name>
		<url-pattern>/profile</url-pattern>
	</servlet-mapping>

